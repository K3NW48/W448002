call plug#begin()

Plug 'SirVer/ultisnips' | Plug 'honza/vim-snippets'
Plug 'mattn/emmet-vim'
Plug 'wakatime/vim-wakatime'

call plug#end()

set mouse=a
colorscheme murphy
set spell
set title
set cursorline
set number
set ruler
syntax enable
set hlsearch
set ignorecase
set smartcase
set autoindent
set history=4300
set smarttab
set tabstop=2
set paste
