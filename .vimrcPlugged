call plug#begin()

Plug 'https://github.com/prettier/vim-prettier.git'
Plug 'rdnetto/YCM-Generator' { 'branch': 'stable' }
Plug 'SirVir/ultisnips' | Plug 'honza/vim-snippets'
Plug 'preservim/nerdtree'


call plug#end()
set mouse=a
colorscheme industry
set spell
set title
set cursorline
set number
set ruler
syntax enable
set hlsearch
set ignorecase
set smartcase
set autoindent
set history=4300
set smarttab
set tabstop=2
set paste
